{
  "version": 2,
  "builds": [
    {
      "src": "matha/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "60xmb", "runtime": "python3.9" }
    },
    { "src": "package.json", "use": "@vercel/static-build" }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "matha/wsgi.py"
    },
    { "src": "/(.*)", "dest": "src/index.js" }
  ]
}